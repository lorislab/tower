<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">

	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<!-- USER MANAGEMENT -->
		<navigation-case>
			<from-outcome>toUser</from-outcome>
			<to-view-id>/pages/userman/user/search.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>toRole</from-outcome>
			<to-view-id>/pages/userman/role/search.xhtml</to-view-id>
			<redirect />
		</navigation-case>	
		<!-- SETTINGS -->	
		<navigation-case>
			<from-outcome>toAgent</from-outcome>
			<to-view-id>/pages/settings/agent/search.xhtml</to-view-id>
			<redirect />
		</navigation-case>		
		<navigation-case>
			<from-outcome>toApplication</from-outcome>
			<to-view-id>/pages/settings/application/search.xhtml</to-view-id>
			<redirect />
		</navigation-case>						
		<navigation-case>
			<from-outcome>toBTS</from-outcome>
			<to-view-id>/pages/settings/bts/search.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>toMail</from-outcome>
			<to-view-id>/pages/settings/mail/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>toProject</from-outcome>
			<to-view-id>/pages/settings/project/search.xhtml</to-view-id>
			<redirect />
		</navigation-case>	
		<navigation-case>
			<from-outcome>toSCM</from-outcome>
			<to-view-id>/pages/settings/scm/search.xhtml</to-view-id>
			<redirect />
		</navigation-case>	
		<navigation-case>
			<from-outcome>toSystem</from-outcome>
			<to-view-id>/pages/settings/system/search.xhtml</to-view-id>
			<redirect />
		</navigation-case>	
		<navigation-case>
			<from-outcome>toTimer</from-outcome>
			<to-view-id>/pages/settings/timer/edit.xhtml</to-view-id>
			<redirect />
		</navigation-case>						
		<!-- DASHBOARD -->			
		<navigation-case>
			<from-outcome>toDashboard</from-outcome>
			<to-view-id>/pages/dashboard/dashboard.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<!-- DEPLOYMENT -->
		<navigation-case>
			<from-outcome>toDeploy</from-outcome>
			<to-view-id>/pages/deploy/deploy.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		<!-- OTHER -->
		<navigation-case>
			<from-outcome>toProfile</from-outcome>
			<to-view-id>/pages/profile/profile.xhtml</to-view-id>
			<redirect />
		</navigation-case>		
		<navigation-case>
			<from-outcome>toHome</from-outcome>
			<to-view-id>/pages/dashboard/dashboard.xhtml</to-view-id>
			<redirect />
		</navigation-case>				
	</navigation-rule>
</faces-config>
